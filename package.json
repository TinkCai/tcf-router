{
  "name": "tcf-router",
  "version": "0.0.8",
  "description": "provide a express-like tencent cloudbase function router and a local simulator",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tcf": "./bin/index.js"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "jest --coverage",
    "build": "tsc",
    "dev": "ts-node client/server.ts ./demo",
    "format": "prettier --config .prettierrc.json --write \"{src,client,test}/**/*.{j,t}s\""
  },
  "keywords": [
    "cloudbase",
    "tencent",
    "lambda",
    "serverless",
    "wechat",
    "mini-program"
  ],
  "author": "Tink Cai",
  "license": "ISC",
  "devDependencies": {
    "@cloudbase/types": "^2.0.0-alpha.2",
    "@types/express-serve-static-core": "^4.17.29",
    "@types/node": "^18.0.0"
  },
  "dependencies": {
    "@cloudbase/js-sdk": "^1.7.2",
    "@types/cookie": "^0.5.1",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "body-parser": "^1.20.0",
    "commander": "^9.3.0",
    "cookie": "^0.5.0",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "inquirer": "^9.0.0",
    "jsonwebtoken": "^8.5.1",
    "path-to-regexp": "^6.2.1",
    "prettier": "2.3.2",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
