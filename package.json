{
  "name": "tcf-router",
  "version": "0.1.2",
  "description": "provide a express-like tencent cloudbase function router and a local simulator",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "bin": {
    "tcf": "./bin/index.js"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "jest --coverage",
    "build": "tsc",
    "clean": "tsc --build --clean",
    "dev": "cd demo/ts-project && tcf dev",
    "format": "prettier --config .prettierrc.json --write \"{src,client,test}/**/*.{j,t}s\""
  },
  "repository": {
    "github": "https://github.com/TinkCai/tcf-router"
  },
  "keywords": [
    "cloudbase",
    "tencent",
    "lambda",
    "serverless",
    "wechat",
    "mini-program"
  ],
  "author": "Tink Cai",
  "license": "ISC",
  "devDependencies": {
    "@cloudbase/types": "^2.0.0-alpha.2",
    "@types/express-serve-static-core": "^4.19.3",
    "@types/node": "^20.14.2"
  },
  "dependencies": {
    "@cloudbase/manager-node": "^4.2.9",
    "@types/cookie": "^0.6.0",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "body-parser": "^1.20.2",
    "commander": "^12.1.0",
    "cookie": "^0.6.0",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "inquirer": "^9.2.23",
    "jsonwebtoken": "^9.0.2",
    "path-to-regexp": "^6.2.2",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "zip-local": "^0.3.5"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
